{
  "name": "midway-vue3-ssr",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "dependencies": {
    "@midwayjs/bootstrap": "^3.0.0",
    "@midwayjs/core": "^3.0.0",
    "@midwayjs/decorator": "^3.0.0",
    "@midwayjs/info": "^3.0.0",
    "@midwayjs/koa": "^3.0.0",
    "@midwayjs/logger": "^2.14.0",
    "@midwayjs/mikro": "^3.5.3",
    "@midwayjs/redis": "^3.5.3",
    "@midwayjs/static-file": "^3.5.3",
    "@midwayjs/typegoose": "^3.5.3",
    "@midwayjs/typeorm": "^3.5.3",
    "@midwayjs/validate": "^3.0.0",
    "@mikro-orm/core": "^5.4.2",
    "@mikro-orm/mysql": "^5.4.2",
    "@typegoose/typegoose": "^9.12.1",
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "mongoose": "6.6.3",
    "mysql2": "^2.3.3",
    "pinia": "^2.0.21",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@midwayjs/cli": "^1.2.90",
    "@midwayjs/cli-plugin-swc": "^1.3.13",
    "@midwayjs/mock": "^3.0.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@swc/core": "^1.3.4",
    "@types/jest": "^26.0.10",
    "@types/koa": "^2.13.4",
    "@types/lodash": "^4.14.186",
    "@types/node": "^16.11.64",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "cross-env": "^6.0.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jest": "^26.4.0",
    "koa-connect": "^2.1.0",
    "mwts": "^1.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-jest": "^26.2.0",
    "typescript": "~4.7.4",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.7"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node ./bootstrap.js",
    "dev": "cross-env NODE_ENV=local midway-bin dev --ts --fast=swc",
    "build": "run-p build:midway build:view",
    "build:midway": "midway-bin build -c",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "run-p lint:midway lint:view",
    "lint:midway": "mwts fix",
    "ci": "npm run cov",
    "dev:view": "vite view",
    "preview:view": "vite view preview --port 4173",
    "build:view": "run-p type-check build-only:view",
    "build-only:view": "run-p build:view:client build:view:server",
    "build:view:client": "vite build view --outDir ../public/client --ssrManifest",
    "build:view:server": "vite build view --outDir ../public/server --ssr src/server",
    "type-check": "vue-tsc -p view/tsconfig.json --noEmit",
    "lint:view": "eslint view -c ./view/.eslintrc.cjs --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "clean": "midway-bin clean"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist",
    "public",
    "logs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ilyydy/midway-vue3-ssr"
  },
  "author": "zhangchi@bigo.sg",
  "license": "MIT"
}
